  "[cpp]": {
    "editor.wordBasedSuggestions": "matchingDocuments",
    "editor.suggest.insertMode": "insert",
    "editor.semanticHighlighting.enabled": true,
    "cSpell.fixSpellingWithRenameProvider": true,
    "editor.formatOnPaste": true,
    "editor.rulers": [{ "color": "ðŸŸ¡", "column": 120 }],
    "breadcrumbs.showArrays": true,
    "breadcrumbs.showBooleans": true,
    "breadcrumbs.showClasses": true,
    "breadcrumbs.showConstants": true,
    "breadcrumbs.showConstructors": true,
    "breadcrumbs.showEnumMembers": true,
    "breadcrumbs.showEnums": true
  },
  "clang.completion.enable": true,
  "clang.completion.maxBuffer": 8388608,
  "clang.completion.triggerChars": [".", ":", ">"],
  "clang.diagnostic.enable": true,
  "clang.diagnostic.delay": 200,
  "C_Cpp.autocomplete": "default",
  "C_Cpp.autocompleteAddParentheses": true,
  "C_Cpp.clang_format_style": "../.clang-format",
  "C_Cpp.clang_format_path": "clang-format",
  "C_Cpp.configurationWarnings": "enabled",
  "C_Cpp.inactiveRegionOpacity": 0.4,
  "C_Cpp.inlayHints.autoDeclarationTypes.showOnLeft": true,
  "C_Cpp.inlayHints.parameterNames.enabled": true,
  "C_Cpp.inlayHints.referenceOperator.enabled": true,
  "C_Cpp.vcFormat.indent.withinParentheses": "alignToParenthesis",
  "C_Cpp.suggestSnippets": true,
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
  "cSpell.words": ["Polynomialable"],
  "cmake.ctest.allowParallelJobs": true,
  "cmake.configureOnEdit": true,
  "cmake.loggingLevel": "info",
  // "C_Cpp.codeAnalysis.clangTidy.checks.enabled": ["*"],
  "C_Cpp.codeAnalysis.clangTidy.codeAction.formatFixes": true,
  "C_Cpp.codeAnalysis.runAutomatically": true,
  "C_Cpp.default.cppStandard": "c++20",
  "C_Cpp.intelliSenseEngine": "default",
}
